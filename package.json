{
  "name": "property-management-vite",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "dev": "react-router dev",
    "start": "react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "eslint --ext .js,.jsx,.ts,.tsx . --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{js,.jsx,ts,tsx,json,css,md}\"",
    "prepare": "husky install",
    "prisma:seed": "npx ts-node-esm --skipIgnore --skipProject --transpileOnly prisma/seed.ts",
    "seed:auth": "NODE_OPTIONS=\"--no-warnings\" npx ts-node-esm --skipIgnore --skipProject --transpileOnly scripts/seed-with-auth.ts"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
